class persona{
	String nombre;
	int edad;
	
	public persona(String name, int age) {
		this.nombre=name;
		this.edad=age;
	}
	
	public String toString() {
		return nombre+"\n"+edad;
	}

}

class hero extends persona{
	String power;
					
	hero(String name,int age, String power){
		super(name,age);
		this.power=power;
		
		
	}
	public String toString() {
		return super.toString()+"\n"+power;
	}
	
}
