	}

}

class Car{
	public String automovil;
	int c;
	Car(String modelo){
		
		automovil=modelo;
		c++;
		
	}
	
}
	
class Garage{
	
	void Park(Car auto){
		System.out.println("el auto "+auto.automovil);
		System.out.println(auto.c);
	}
	
}
