class cuadrado{
	private static float lado;
	cuadrado(float num){
		lado=num;
		
}		
	cuadrado(){
		lado=0;
	}
	
	void set_cuadrado(float num){
		
		lado=num;
	}
	
	float get_cuadrado(){
		return lado;
	}
	
	float superficie(){
		return lado*lado;
		
	}
	
	
	String tostring(){
		return "la superficie de: "+lado +" es igual a "+superficie();
		
		
	}






