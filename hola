class car{
	
	public String nombre;
	public int edad;
	
	public car(String nom,int num1) {
		this.nombre = nom;
		this.edad  =  num1;
	}
	
	public String getName(){
		return nombre;
	}
	
	public int getAge() {
		return edad;
	}
	
	public String toString() {
		
		return "el nombre del sujeto es "+nombre+" y su edad es de "+edad;
	}
}


