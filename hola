        elif op == 5:
            buscar_ticket()

        elif op == 6:
            conteo()

        elif op == 7:
            conteo_por_filas()

        elif op == 8:
            v = []
            prom = promedio()
            v = calcular(prom, v)

            ordenamiento(v)
            for i in range(len(v)):
                print(str(v[i])+" "+str(patente(v[i].patente)))


