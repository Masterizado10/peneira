class humano{
	
	
	humano(String name,int edad){
		String nombre;
		int age;
		nombre=name;
		age=edad;
		comer (nombre,age);
		
		
	}
	
	public void comer(String nombre,int age) {
		
		System.out.println(nombre+" esta comiendo");
		System.out.println("su edad es de "+age);
		
	}

